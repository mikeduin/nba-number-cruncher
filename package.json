{
  "name": "server",
  "version": "1.0.0",
  "main": "app.js",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "type": "module",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register app.js",
    "server": "nodemon --exec ts-node -r tsconfig-paths/register app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.12",
    "chrome-aws-lambda": "^10.1.0",
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "dotenv": "^8.1.0",
    "express": "^4.16.4",
    "knex": "^0.21.1",
    "lodash": "^4.17.20",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.1",
    "nodemon": "^1.18.9",
    "pg": "^8.5.1",
    "prettier": "^1.16.1",
    "prop-types": "^15.6.2",
    "puppeteer": "^21.6.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^4.0.1",
    "react-table": "^6.9.0",
    "semantic-ui-react": "^0.85.0",
    "semver": "^5.6.0",
    "serve-favicon": "^2.5.0",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^0.13.1",
    "typescript": "^5.6.2",
    "victory": "^31.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.6",
    "@babel/core": "^7.25.2",
    "@babel/node": "^7.25.0",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.7",
    "@types/node-schedule": "^2.1.7",
    "pg-connection-string": "^2.5.0",
    "ts-node": "^10.9.2"
  },
